<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_faculty_faculty" model="ir.sequence">
            <field name="name">faculty Sequence</field>
            <field name="code">faculty.faculty.sequence</field>
            <field name="prefix">S</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <data noupdate="1">

        <record id="seq_student_student" model="ir.sequence">
            <field name="name">student Sequence</field>
            <field name="code">student.student.sequence</field>
            <field name="prefix">S</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>
